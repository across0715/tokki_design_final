<header class="navbar">
  <div class="navbar__logo">
    <div class="logo_set">
      <%= image_tag 'tkimg/icon_white_s.png' %>
      <i class="fas fa-carrot"></i>
    </div>
  </div>
  <ul class ="navbar__menu" >
    <li><%= link_to "Home",      root_path,   id: "logo_2"%></li>
    <li><%= link_to "About",     about_path , id: "logo_2"%></li>
    <li><%= link_to "Gallery",   gallery_path,id: "logo_2"%></li>
    <li><%= link_to "FAQ",       faq_path,    id: "logo_2"%></li>
    <% if logged_in? %>
      <li><%= link_to "Users", users_path ,id: "logo_2"%></li>
      <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" id="account">
          Account <b class="caret"></b>
        </a>
        <ul class="dropdown-menu">
          <li><%= link_to "Profile", current_user %></li>
          <li><%= link_to "Settings", edit_user_path(current_user) %></li>
          <li>
            <%= link_to "Log out", logout_path, method: :delete %>
          </li>
        </ul>
      </li>
    <% else %>
      <li><%= link_to "Log in", login_path ,id: "logo_2"%></li>
      <li><%= link_to "Sign up", signup_path,  id: "logo_2"%></li>
    <% end %>
  </ul>
</header>
